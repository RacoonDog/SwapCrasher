{
  "schemaVersion": 1,
  "id": "swap-crasher",
  "version": "${version}",
  "name": "Swap Crasher",
  "description": "A Meteor Client addon that implements the swap crash.",
  "authors": [
    "Crosby"
  ],
  "contact": {
    "issues": "https://github.com/RacoonDog/SwapCrasher/issues",
    "sources": "https://github.com/RacoonDog/SwapCrasher"
  },
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.racoondog.crasher.Crasher"
    ]
  },
  "mixins": [
    "swap-crasher.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "255,85,255"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
